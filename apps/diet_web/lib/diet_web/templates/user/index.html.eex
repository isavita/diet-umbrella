<h1>Dieting Users:</h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Actions</th>
  </tr>
  <%= for user <- @users do %>
    <tr>
      <td>
        <%= render "user.html", user: user %>
      </td>
      <td>
        <%= link "View", to: Routes.user_path(@conn, :show, user.id) %>
      </td>
    </tr>
  <% end %>
</table>
