<div class="newsfeed">
  <h2>The Newest Videos</h2>
  <ul class="newsfeed-popular-videos">
  <%= for video <- @videos do %>
    <li>
      <div>
        <h4><%= video.title %><h4>
        <iframe class="newsfeed-iframe-player" src="<%= embedded_url(video) %>" allowfullscreen>
        </iframe>
      </div>
    </li>
  <% end %>
  </ul>
</div>
