<div class="container mt-5">
  <div class="row">
    <div class="col-md-4 order-md-2">
      <%= render "coming_soon.html", assigns %>
      <%= render "newest_posts.html", assigns %>
    </div>

    <div class="col-md-8 order-md-1">
      <%= for video <- @videos do %>
        <%= render "video.html", Map.put(assigns, :video, video) %>
      <% end %>
    </div>
  </div>
</div>
